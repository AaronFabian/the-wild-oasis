# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true

[*.jsx]
charset = utf-8
indent_size = 2
indent_style = tab
insert_final_newline = true

[*.js]
charset = utf-8
indent_size = 2
indent_style = tab

[*.html]
charset = utf-8
indent_size = 2
indent_style = tab
